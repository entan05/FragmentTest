<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="jp.team.e_works.fragmenttest.FragmentTest02Activity">

    <include
        android:id="@+id/fragment_test02_switchs"
        layout="@layout/switchs"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/fragment_test02_switchs"
        android:orientation="horizontal">

        <RelativeLayout
            android:id="@+id/Fragment_test02_container01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/Fragment_test02_container02"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">
        </RelativeLayout>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>
